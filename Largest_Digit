import java.util.*;
class Largest_Digit{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int s = sc.nextInt();
        int h =0;
        while(s>0){
            int t = s%10;
            if(t>h){
                h =t;
            }
            s /= 10;
        }
        System.out.println();
        System.out.println(h);
    }
}
